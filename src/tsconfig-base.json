{
    "compilerOptions": {
        "rootDir": ".",
        "outDir": "../build/tsjs",
        "target": "ES5",
        "composite": true,
        "lib": ["DOM", "ES5", "ES2015"],
        "sourceMap": true,
        "incremental": true,
        "noEmitOnError": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
