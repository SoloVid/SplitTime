{
  "compilerOptions": {
    "moduleResolution": "node16",
    "module": "CommonJS",
    "isolatedModules": true,
    "lib": ["DOM", "ES5", "ES2015"],
    "sourceMap": true,
    "incremental": false,
    "noEmitOnError": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "paths": {
      "spt": ["./src/engine/splitTime"],
      "assets": ["./src/engine/assets/assets"]
    }
  }
}